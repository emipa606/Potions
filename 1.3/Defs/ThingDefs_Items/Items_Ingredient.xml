    <defName>PlasteelDust</defName>
    <label>plasteel dust</label>
    <description>Advanced spacer tech material brought to medieval times. Plasteel dust is used as a ingredient in potion brewing. Not very strong.</description>
    <graphicData>
      <texPath>Things/Item/PlasteelDust</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stackLimit>500</stackLimit>
    <statBases>
      <MaxHitPoints>0.2</MaxHitPoints>
      <MarketValue>0.9</MarketValue>
      <Mass>0.008</Mass>
      <Flammability>0.2</Flammability>
    </statBases>
    <thingCategories>
      <li>
        <!--Manufactured-->PIngredients</li>
    </thingCategories>
  </ThingDef>
