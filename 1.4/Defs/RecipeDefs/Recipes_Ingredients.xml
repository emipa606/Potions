<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef>
    <defName>BottleCrafting</defName>
    <label>smelt stone chunks into bottles</label>
    <description>Turn the stone chunks into bottles used for potion brewing.</description>
    <jobString>Smelting stone chunks into bottles.</jobString>
    <workAmount>1600</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>StoneChunks</li>
          </categories>
        </filter>
        <count>0.25</count>
      </li>
    </ingredients>
    <products>
      <PGlassBottle>3</PGlassBottle>
    </products>
    <skillRequirements>
      <Crafting>3</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <fixedIngredientFilter>
      <categories>
        <li>StoneChunks</li>
      </categories>
    </fixedIngredientFilter>
    <researchPrerequisite>Smithing</researchPrerequisite>
    <recipeUsers>
      <li>FueledSmithy</li>
      <li>ElectricSmithy</li>
    </recipeUsers>
  </RecipeDef>
