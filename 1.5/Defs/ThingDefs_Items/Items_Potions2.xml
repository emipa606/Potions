<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--================Potion of Stats==================-->
  <!--==Pain==-->
  <ThingDef>
    <defName>PPotionPain</defName>
    <label>Pain potion of stats</label>
    <description>A potion that decreases the pain for short time.</description>
    <graphicData>
      <texPath>Things/Item/PotionStats</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <DeteriorationRate>0.25</DeteriorationRate>
      <MarketValue>40</MarketValue>
      <Mass>0.3</Mass>
      <WorkToMake>450</WorkToMake>
      <Nutrition>0.05</Nutrition>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Beauty>0</Beauty>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
      <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
      <ingestEffect>DrinkPotion</ingestEffect>
      <!--<ingestSound>Ingest_Pill</ingestSound>-->
      <foodType>Meal</foodType>
      <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
      <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>PainHigh</hediffDef>
          <severity>1.00</severity>
          <doToGeneratedPawnIfAddicted>true</doToGeneratedPawnIfAddicted>
        </li>
      </outcomeDoers>
      <ingestCommandString>Drink {0}</ingestCommandString>
      <ingestReportString>Drinking {0}.</ingestReportString>
    </ingestible>
    <uiIconForStackCount>1</uiIconForStackCount>
    <pathCost>15</pathCost>
    <resourceReadoutPriority>Last</resourceReadoutPriority>
    <drawGUIOverlay>true</drawGUIOverlay>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li Class="CompProperties_Ingredients" />
      <li Class="CompProperties_FoodPoisonable" />
    </comps>
    <alwaysHaulable>true</alwaysHaulable>
    <thingClass>ThingWithComps</thingClass>
    <category>Item</category>
    <useHitPoints>true</useHitPoints>
    <selectable>true</selectable>
    <altitudeLayer>Item</altitudeLayer>
    <stackLimit>10</stackLimit>
    <tickerType>Rare</tickerType>
    <socialPropernessMatters>true</socialPropernessMatters>
    <thingCategories>
      <li>
        <!--FoodMeals-->PPotionsStats</li>
    </thingCategories>
    <techLevel>Medieval</techLevel>
  </ThingDef>
